{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "640",
			"wideSize": "1080px"
		},
		"spacing": {
			"units": [ "%", "px", "em", "rem", "vh", "vw" ],
			"blockGap": true
		},
		"color": {
			"palette": [
                {
                    "name": "Off-white",
                    "slug": "off-white",
                    "color": "#00000018"
                }
            ]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "Afacad, sans-serif",
					"name": "Afacad",
					"slug": "afacad",
					"fontFace": [
						{
							"fontFamily": "Afacad",
							"fontWeight": "400 700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Afacad-VariableFont_Wght.ttf" ]
						},
						{
							"fontFamily": "Afacad",
							"fontWeight": "400 700",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Afacad-Italic-VariableFont_Wght.ttf" ]
						}
					]
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"css": "",
		"spacing": {
			"blockGap": "1.5rem"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "Header"
		},
		{
			"area": "footer",
			"name": "Footer"
		}
	],
	"version": 3
}
